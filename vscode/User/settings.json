{
    "workbench.iconTheme": "material-icon-theme",
    "[cpp]": {
        "editor.quickSuggestions": false
    },
    "[c]": {
        "editor.quickSuggestions": false
    },
    // "workbench.colorTheme": "Atom One Dark",
    // "editor.fontFamily": "Iosevka SS08",
    "editor.fontFamily": "Victor Mono",
    // "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "editor.fontSize": 18,
    "editor.tabSize": 2,
    "editor.rulers": [
        80
    ],
    "editor.cursorBlinking": "phase",
    "editor.fontLigatures": false,
    "editor.renderLineHighlight": "all",
    "editor.formatOnSave": true,
    "files.eol": "\n",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": true,
    "window.zoomLevel": 0,
    "cmake-tools-helper.cmake_download_path": "/home/ahodos/.vscode/extensions/maddouri.cmake-tools-helper-0.2.1/cmake_download",
    "rust-client.channel": "stable",
    "workbench.colorTheme": "Community Material Theme Darker",
    "rust-analyzer.highlightingOn": true,
    "lldb.showDisassembly": "never",
    "lldb.launch.sourceLanguages": [
        "cpp",
        "rust"
    ],
    // "rust-analyzer.rainbowHighlightingOn": true,
}